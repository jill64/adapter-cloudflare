{
  "name": "@jill64/adapter-cloudflare",
  "version": "0.1.1",
  "description": "ðŸ”Œ SvelteKit Adapter for Cloudflare Pages with node_compat",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/adapter-cloudflare/issues",
  "homepage": "https://github.com/jill64/adapter-cloudflare#readme",
  "files": [
    "dist",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/adapter-cloudflare.git"
  },
  "keywords": [
    "adapter",
    "cloudflare",
    "node",
    "pages",
    "sveltekit"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "pnpm build:lib && cd demo && pnpm dev",
    "build": "pnpm build:lib && pnpm build:demo",
    "build:lib": "tsc && npx publint",
    "build:demo": "cd demo && pnpm build",
    "preview": "pnpm build:lib && cd demo && pnpm preview",
    "prepack": "pnpm build",
    "lint": "pnpm build:lib && pnpm lint:lib && pnpm lint:demo",
    "lint:lib": "npx eslint .",
    "lint:demo": "cd demo && pnpm lint",
    "format": "pnpm format:lib && pnpm format:demo",
    "format:lib": "npx prettier -w .",
    "format:demo": "cd demo && pnpm format",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.0.0",
    "@jill64/playwright-config": "1.0.0",
    "@jill64/prettier-config": "1.0.0",
    "@playwright/test": "1.40.1",
    "@sveltejs/kit": "1.27.7",
    "@types/node": "20.10.3",
    "@types/ws": "8.5.10",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "@cloudflare/workers-types": "4.20231121.0",
    "@sveltejs/adapter-cloudflare": "2.3.4",
    "esbuild-plugin-polyfill-node": "0.3.0",
    "esbuild": "0.19.8",
    "import-meta-resolve": "4.0.0",
    "worktop": "0.8.0-next.15"
  }
}